<com.example.cootek.newfastframe.view.DragLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/dl_activity_main_drag"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/wallpaper">

    <include layout="@layout/view_activity_main_menu" />


    <com.example.cootek.newfastframe.view.slide.SlidingPanelLayout
        android:id="@+id/sl_activity_main_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/base_color_text_white"
        android:gravity="bottom"
        android:orientation="vertical"
        app:umanoOverlay="false"
        app:umanoPanelHeight="60dp"
        app:umanoShadowHeight="5dp">


        <FrameLayout
            android:id="@+id/fl_activity_main_content"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <FrameLayout
            android:id="@+id/fl_activity_main_bottom"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

    </com.example.cootek.newfastframe.view.slide.SlidingPanelLayout>


</com.example.cootek.newfastframe.view.DragLayout>


